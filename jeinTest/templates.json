ccm.callback["templates.json"]({
    "questions": {
        "tag": "div",
        "class": "questions"
    },
    
    "question": {
        "tag": "div",
        "class": "question",
        "data-qid": "%qid%",
        "inner": [
            {
                "tag": "div",
                "class": "text",
                "inner": "%text%"
            },
            {
                "tag":"button",
                "class":"btnWahr",
                "data-btnid":"%qid%",
                "onclick":"%onsubmittrue%",
                "inner":"WAHR"
            },
            {
                "tag":"button",
                "class":"btnFalsch",
                "data-btnid":"%qid%",
                "onclick":"%onsubmitfalse%",
                "inner":"FALSCH"
            }
        ]
    },
    
    "start": {
        "tag":"div",
        "class":"start",
        "inner": [   
            {
                "tag":"div",
                "class":"starttext",
                "inner":"%starttext%"
            },        
            {
                "tag":"button",
                "class":"btnStart",
                "onclick":"%onclickstart%",
                "inner":"START"
            },
            {
                "tag":"button",
                "class":"btnEdit",
                "onclick":"%onclickedit%",
                "inner":"Edit"
            },
            {
                "tag":"button",
                "class":"btnLogin",
                "onclick":"%onclicklogin%",
                "inner":"Login"
            }
        ]
    },
    
    "results": {
        "tag":"div",
        "class":"results",
        "inner":[
            {
                "tag":"div",
                "class":"result",
                "inner":"%result%"
            },
            {
                "tag":"button",
                "class":"btnRestart",
                "onclick":"%onclickrestart%",
                "inner":"Okay!"
            }
        ]
    },
    
    "questionCheckbox": {
        "tag":"label",
        "inner":[
            {
                "tag":"input",
                "type":"checkbox",
                "class":"qCheckbox",
                "data-chkid":"%chkid%",
                "checked":"%checked%",
                "inner":"Wahr"
            },
            {
                "tag":"span",
                "inner":"Wahr"
            }
        ]
    },
    
    "editButtons":{
        "tag":"div",
        "class":"editButtons",
        "inner": [
            {
                "tag":"button",
                "class":"btnAddNew",
                "onclick":"%onclickAddNew%",
                "inner":"Neue Frage"
            },

            {
                "tag":"button",
                "class":"btnApply",
                "onclick":"%onclickApply%",
                "inner":"Speichern"
            },
            
            {
                "tag":"button",
                "class":"btnCancel",
                "onclick":"%onclickCancel%",
                "inner":"Abbrechen"
            }
        ]
    }
})

